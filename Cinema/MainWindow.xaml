<Window x:Class="Cinema.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cinema"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1500">
    <Grid Background="#FFCBD1B5">
        <Grid.RowDefinitions>
           
            <RowDefinition Height="274*"/>
            <RowDefinition Height="22*"/>

        </Grid.RowDefinitions>
        <Button x:Name="BackBtn" HorizontalAlignment="Right"  Style="{StaticResource Button}" Click="BackBtn_Click" Grid.Row="1">Назад</Button>
       
        <Frame Grid.Row="0" x:Name="MyFrame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
